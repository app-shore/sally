{
  "name": "@sally/studio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "studio": "remotion studio src/index.ts",
    "render": "remotion render src/index.ts SallyLaunchVideo out/sally-launch.mp4",
    "render:webm": "remotion render src/index.ts SallyLaunchVideo out/sally-launch.webm --codec vp8",
    "publish": "cp out/sally-launch.mp4 ../web/public/videos/sally-launch.mp4 && cp out/sally-launch.webm ../web/public/videos/sally-launch.webm",
    "preview": "remotion preview src/index.ts"
  },
  "dependencies": {
    "@remotion/cli": "4.0.261",
    "@remotion/google-fonts": "^4.0.421",
    "@remotion/transitions": "^4.0.421",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "remotion": "4.0.261"
  },
  "devDependencies": {
    "@types/react": "^18.3.0",
    "typescript": "^5.9.0"
  }
}
